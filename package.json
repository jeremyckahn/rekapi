{
  "name": "rekapi",
  "version": "2.3.0",
  "homepage": "http://rekapi.com",
  "author": "Jeremy Kahn <jeremyckahn@gmail.com>",
  "description": "A keyframe animation library for JavaScript",
  "main": "dist/rekapi.js",
  "contributors": [
    {
      "name": "Franck Lecollinet"
    },
    {
      "name": "Brian Downing"
    }
  ],
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@jeremyckahn/minami": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "c8": "^7.11.3",
    "concurrently": "^3.5.0",
    "gh-pages": "^1.0.0",
    "globals": "^16.3.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^24.0.0",
    "live-server": "^1.2.0",
    "lodash": "^4.17.21",
    "node-abort-controller": "^1.1.0",
    "nodemon": "^1.11.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "vite build",
    "ci": "npm test && npm run lint",
    "start": "vite dev",
    "test": "c8 vitest run",
    "test:watch": "vitest --watch",
    "doc": "jsdoc -c .jsdoc src/**/*.ts",
    "doc:view": "live-server dist/doc --port=9124",
    "doc:watch": "nodemon --exec \"npm run doc\" --watch src --watch ./ --ext ts,md --ignore dist",
    "doc:live": "concurrently --kill-others \"npm run doc:watch\" \"npm run doc:view\"",
    "lint": "eslint . --ext .ts",
    "deploy": "npm run build && npm run doc && gh-pages -d dist -b gh-pages",
    "preversion": "npm run lint && npm test"
  },
  "files": [
    "src",
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash.sortedindexby": "^4.6.0",
    "shifty": "^2.20.0"
  }
}
